{
  "name": "poppy-casting",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@rescript/react",
    "rescript-react-intl",
    "@ryyppy/rescript-promise",
    "@mobily/rescript-date",
    "rescript-webapi",
    "@rescript/core",
    "@greenlabs/rescript-react-hook-form",
    "@rescript-mui/material"
  ],
  "bsc-flags": ["-open RescriptCore"],
  "jsx": { "version": 4, "mode": "automatic" },
  "uncurried": false
}
